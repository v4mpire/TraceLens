{
  "name": "tracelens-web",
  "description": "TraceLens Modern UI Dashboard",
  "version": "1.0.0",
  "type": "dockerfile",
  "repository": "https://github.com/v4mpire/TraceLens.git",
  "branch": "main",
  "buildCommand": "npm run build",
  "dockerfile": "apps/web/Dockerfile",
  "port": 3000,
  "healthCheck": "/",
  "resources": {
    "cpu": "0.5",
    "memory": "1Gi",
    "storage": "5Gi"
  },
  "monitoring": {
    "enabled": true,
    "healthChecks": true,
    "logs": true
  }
}
