{
  "name": "tracelens-full-stack",
  "description": "TraceLens Complete Observability Platform",
  "version": "1.0.0",
  "type": "docker-compose",
  "repository": "https://github.com/v4mpire/TraceLens.git",
  "branch": "main",
  "dockerComposeFile": "docker-compose.yaml",
  "services": {
    "web": {
      "port": 3000,
      "healthCheck": "/api/health"
    },
    "ingestion-service": {
      "port": 3001,
      "healthCheck": "/health"
    }
  },
  "resources": {
    "cpu": "1.0",
    "memory": "2Gi",
    "storage": "10Gi"
  },
  "monitoring": {
    "enabled": true,
    "healthChecks": true,
    "logs": true
  },
  "environment": {
    "POSTGRES_PASSWORD": "generate_secure_password",
    "API_URL": "https://your-domain.com",
    "LOG_LEVEL": "info"
  }
}
