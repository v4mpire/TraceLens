  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/v4mpire/TraceLens.git",
    "directory": "packages/browser-sdk"
  },
  "homepage": "https://github.com/v4mpire/TraceLens#readme",
  "bugs": {
    "url": "https://github.com/v4mpire/TraceLens/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@tracelens/eslint-config": "file:../../tools/eslint-config",
    "@tracelens/tsconfig": "file:../../tools/tsconfig",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
