{
  "name": "@tracelens/ingestion-service",
  "version": "0.1.0",
  "description": "Event processing and data normalization service",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tracelens/shared": "file:../shared",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "pg": "^8.11.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
    "@tracelens/eslint-config": "file:../../tools/eslint-config",
    "@tracelens/tsconfig": "file:../../tools/tsconfig",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/compression": "^1.7.5",
    "@types/pg": "^8.10.0",
    "@types/node": "^20.10.5",
    "@types/jest": "^29.0.0",
    "typescript": "^5.0.0",
    "tsx": "^4.6.2",
    "jest": "^29.0.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src"],
    "testMatch": ["**/__tests__/**/*.test.ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/__tests__/**"
    ]
  }
}
