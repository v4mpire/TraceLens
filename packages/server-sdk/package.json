  "description": "TraceLens Server SDK - Backend tracing and dependency analysis with OpenTelemetry",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "echo 'Using pre-built dist files'",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
