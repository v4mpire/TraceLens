    "type-check": "tsc --noEmit",
    "prepublishOnly": "echo 'Using pre-built dist files'"
  },
  "keywords": [
    "tracing",
    "observability",
    "opentelemetry",
    "performance",
    "monitoring",
    "backend",
    "server",
    "express",
    "fastify"
